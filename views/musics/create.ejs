<h1>Create New Music</h1>
<form action="/musics" method="POST">
    <div class="form-group">
        <label for="nome">Nome:</label>
        <input type="text" class="form-control" id="nome" name="nome" required>
    </div>
    <div class="form-group">
        <label for="genero">Gênero:</label>
        <input type="text" class="form-control" id="genero" name="genero" required>
    </div>
    <div class="form-group">
        <label for="duracao">Duração:</label>
        <input type="text" class="form-control" id="duracao" name="duracao" required>
    </div>
    <div class="form-group">
        <label for="autores">Autores:</label>
        <input type="text" class="form-control" id="autores" name="autores" required>
    </div>
    <div class="form-group">
        <label for="ritmo">Ritmo:</label>
        <input type="text" class="form-control" id="ritmo" name="ritmo" required>
    </div>
    <div class="form-group">
        <label for="instru_vocal">Instrumental e Vocal:</label>
        <input type="text" class="form-control" id="instru_vocal" name="instru_vocal" required>
    </div>
    <button type="submit" class="btn btn-primary">Create</button>
</form>
